<!DOCTYPE html>
<html>
	<head>
    <title> all the romans </title>
    <%- include('./partials/head.ejs')%>

	</head>
	<body>
		<h1>The Honored Champions of Rome!</h1>
		<ul>
			<% roman.forEach(roman => { %>
			<li><a href="/Homepage/<%= roman._id; %>"> The Great And Powerful <%= roman.name; %>!</a>
        <form action="/Homepage/<%= roman.id %>?_method=DELETE" method="POST">
          <input type="submit" value="DELETE" />
        </form>
        <a href="/homepage/<%=roman._id %>/edit">Revise Their Story</a>
      </li>
			<% }); %>

		</ul>
    <nav>
      <a href="/Homepage/new">Add to the Hallowed Halls</a>
    </nav>
	</body>
</html>